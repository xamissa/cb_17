            <field name="name">Design Name</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.art.function.use</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Product Art Function Use found
                </p>
            </field>
        </record>

        <menuitem id="carrol_product_art_menu" action="carrol_product_art_action" parent="stock.menu_stock_inventory_control"/>

