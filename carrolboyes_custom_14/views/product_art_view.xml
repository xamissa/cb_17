<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="carrol_product_art_form_view" model="ir.ui.view">
            <field name="name">product.art.function.use.form</field>
            <field name="model">product.art.function.use</field>
            <field name="arch" type="xml">
                <form string="Product Art Function Use">
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="carrol_product_art_tree_view" model="ir.ui.view">
